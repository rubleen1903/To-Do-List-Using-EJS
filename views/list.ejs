<%- include("header") -%>
    <div class="box" id ="heading">

        <h1 style="color:black;">  <%= ListTitle %> To-Do List</h1>
    </div>
      
    <div class="box">
   
        <% for(var i=0; i<newListItems.length; i++) { %>
            
          
   
    <div class="item">
        <input type="checkbox" >
        <p><%= newListItems[i] %></p>
   </div>
   <% } %>  
   <form class="item" method="post" action ="/" >
    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
    <button type="submit" name="button" value = "<%= ListTitle %>">+</button>
    </form>
</div>     
   
<%- include("footer") -%>